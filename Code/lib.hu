extern unsigned long *find(unsigned long, unsigned long*);
#ifdef _WIN32
void sleep_ms(unsigned long milliseconds) {
    Sleep(milliseconds);
}
#else
void sleep_ms(unsigned long milliseconds) {
    struct timespec ts;

    ts.tv_sec = milliseconds / 1000;
    ts.tv_nsec = (milliseconds % 1000) * 1000000;

    nanosleep(&ts, NULL);
}
#endif